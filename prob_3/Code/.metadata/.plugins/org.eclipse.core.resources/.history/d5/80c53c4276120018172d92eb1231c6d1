    //Filtro FIR passa-baixas
    zerarVetores();
    oflag = fir (samples, HLow, out, dbuffer, nx, nh);
    printf("oflag: %u\n",oflag);
    printf("Sinal filtrado com passa baixas\n");
    showOutFIR();

    //Filtro FIR passa-faixa
    zerarVetores();
    oflag = fir (samples, HHigh, out, dbuffer, nx, nh);
    printf("oflag: %u\n",oflag);
    printf("Sinal filtrado com passa faixa\n");

    //Filtro FIR passa-alta
    zerarVetores();
    oflag = fir (samples, HBand, out, dbuffer, nx, nh);
    printf("Sinal filtrado com passa alta\n");


    return 0;
}
void showOutFIR(){
    int i;
    for(i=0; i<NX;i++){
        printf("%d ",out[i]);
    }
}
void zerarVetores(){
    int i;
    for(i=0; i<NH+2; i++){
       dbuffer[i] = 0;
   }
   for(i=0; i<NX; i++){
       out[i] = 0;
   }
}

