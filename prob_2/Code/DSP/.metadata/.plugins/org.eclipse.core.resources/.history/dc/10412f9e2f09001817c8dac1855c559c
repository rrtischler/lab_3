void zerarBuffers(){
    int i;
    for (i= 0; i < 2048; i++){
        outSample[i] = 0;
    }
}

void fft(){
    rfft(samples, NUMNEWSAMPLES, SCALE);
    printf("%d", sizeof(samples)/sizeof(DATA));
}

Int16 CalcBin(){
    Int16 bin = FREQUENCIAAMOSTRAGEM/NUMNEWSAMPLES;
    return bin;
}

Int16 soma(Int16 a, Int16 b){
    long r;
    r = (Int32)a + (Int32)b;
    return sat16(r);
}

Int16 sat16(Int32 r){
    if ( r > UPPERLIMIT)
        r = UPPERLIMIT;
    else if (r < (Int16)-0x8000)
        r = (Int16)-0x8000;
    return (Int16)r;
}

Int16 q_mul(Int16 a, Int16 b){
    Int16 result;
    Int32 temp;
    temp = (Int32)a * (Int32)b; // result type is operand's type
    // Rounding; mid values are rounded up
    temp += K;
    // Correct by dividing by base and saturate result
    result = sat16(temp >> Q);
    return result;
}
